citHeader("To cite in publications use:")

citEntry(
  entry    = "Manual",
  title    = "{{{package}}}",
  author   = "{{{author}}}",
  year     = "{{{year}}}",
  note = "For more information on the qVerse: https://github.com/globalgov/manydata"
  textVersion = paste("{{{author}}}", "{{{package}}}", "{{{year}}}", sep = ".")
)
